{
  "dev": {
    "name": "Development",
    "domain": null,
    "cors": {
      "allowOrigins": ["http://localhost:3000", "https://*.sst.dev"],
      "allowCredentials": true
    },
    "database": {
      "billingMode": "PAY_PER_REQUEST"
    },
    "monitoring": {
      "enabled": false
    }
  },
  "staging": {
    "name": "Staging",
    "domain": null,
    "cors": {
      "allowOrigins": ["https://staging.yourdomain.com"],
      "allowCredentials": true
    },
    "database": {
      "billingMode": "PAY_PER_REQUEST"
    },
    "monitoring": {
      "enabled": true
    }
  },
  "production": {
    "name": "Production",
    "domain": "yourdomain.com",
    "cors": {
      "allowOrigins": ["https://yourdomain.com"],
      "allowCredentials": true
    },
    "database": {
      "billingMode": "PROVISIONED",
      "readCapacity": 5,
      "writeCapacity": 5
    },
    "monitoring": {
      "enabled": true,
      "alarms": true
    }
  }
}